    "@types/webpack-merge": "^4.1.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "ejs": "^2.6.1",
    "gulp": "^4.0.0",
