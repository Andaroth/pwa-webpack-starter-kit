{
  "name": "pwa-webpack-starter-kit",
  "version": "1.0.0",
  "description": "PWA Starter Kit with Webpack",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "gulp": "gulp -f gulp/config.ts --cwd .",
    "start": "BROWSERSLIST=\"edge > 12\" webpack-dev-server --config webpack/dev.config.ts --hot --color --compress --overlay --history-api-fallback --open",
    "build:static": "webpack --config webpack/prod.config.ts",
    "build": "npm run gulp build",
    "serve": "prpl-server --root build/ --host 0.0.0.0 --https-redirect --bot-proxy https://render-tron.appspot.com/render",
    "lint": "npm run gulp lint"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.1.5",
    "@polymer/app-layout": "^3.0.2",
